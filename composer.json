{
    "name": "andrey-helldar/cashier",
    "description": "Cashier provides an expressive, fluent interface to manage billing services.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "bank",
        "billing",
        "cashier",
        "qr"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/cashier/issues",
        "source": "https://github.com/andrey-helldar/cashier"
    },
    "require": {
        "php": "^7.2.5|^8.0",
        "andrey-helldar/support": "^3.18",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "andrey-helldar/cashier-sber-qr": "dev-main",
        "andrey-helldar/cashier-tinkoff-qr": "dev-main",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\Cashier\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "suggest": {
        "andrey-helldar/cashier-sber-qr": "Driver for payment with QR codes via Sber",
        "andrey-helldar/cashier-tinkoff-qr": "Driver for payment with QR codes via Tinkoff"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\Cashier\\ServiceProvider",
                "Helldar\\Cashier\\Providers\\ObserverServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../cashier-sber"
        }, {
            "type": "path",
            "url": "../cashier-tinkoff"
        }
    ]
}
