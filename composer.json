{
    "name": "andrey-helldar/cashier",
    "description": "Cashier provides an expressive, fluent interface to manage billing services.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "bank",
        "billing",
        "cashier"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/cashier/issues",
        "source": "https://github.com/andrey-helldar/cashier"
    },
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.5|^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\Cashier\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
