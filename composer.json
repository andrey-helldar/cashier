{
    "name": "andrey-helldar/cashier",
    "description": "Cashier provides an expressive, fluent interface to manage billing services.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "bank",
        "billing",
        "cashier",
        "payments",
        "subscribe"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/cashier/issues",
        "source": "https://github.com/andrey-helldar/cashier"
    },
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "andrey-helldar/contracts": "^1.11.1",
        "andrey-helldar/simple-data-transfer-object": "^1.0",
        "andrey-helldar/laravel-support": "^2.0",
        "andrey-helldar/support": "^4.4",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "illuminate/console": "^6.0|^7.0|^8.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "psr/http-message": "^1.0",
        "ramsey/uuid": "^3.7|^4.0",
        "symfony/http-kernel": "^4.3.4|^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^9.0",
        "symfony/var-dumper": "^4.3.4|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\Cashier\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "suggest": {
        "andrey-helldar/cashier-sber-qr": "Driver for payment with QR codes via Sber",
        "andrey-helldar/cashier-tinkoff-qr": "Driver for payment with QR codes via Tinkoff"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\Cashier\\Providers\\ServiceProvider",
                "Helldar\\Cashier\\Providers\\ObserverServiceProvider"
            ]
        }
    }
}
